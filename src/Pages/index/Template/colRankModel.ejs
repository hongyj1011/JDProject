<div class="tab-hd clearfix">
        <% for(var i=0; i<rankData.length;i++){ %>
        <% if(i == 0 ){%>
            <a class="tabTitle tab-active">
        <% } else  { %>
            <a class="tabTitle">
        <%}%>
            <%= rankData[i].title %>
        </a>
        <%}%>
</div>
<div class="tab-bd">
    <% for(var i=0; i<rankData.length;i++){ %>
        <% if(i == 0 ){%>
        <div class="tabItem tabItem-active">
        <% } else  { %>
        <div class="tabItem">
        <%}%>
            <div class="swiper-container tab-swiper swiper<%=i%>">
                <div class="swiper-wrapper">
                    <% for(var j=0; j<rankData[i].content.length;j++){ %>
                        <a class="swiper-slide">
                            <img src="<%= rankData[i].content[j].img%>" class="fl tab-icon">
                            <% if(j+1 == 1 ){%>
                                <span class="fl tab-index tab-index-1">
                            <% } else if (j+1 == 2) {%>
                                <span class="fl tab-index tab-index-2">
                            <% } else if (j+1 == 3) {%>
                                <span class="fl tab-index tab-index-3">
                            <% } else  { %>
                                <span class="fl tab-index">
                            <%}%>
                                <%= j+1 %>
                                </span>
                            <span class="fl tab-msg">
                            <%= rankData[i].content[j].msg %>
                            </span>
                        </a>
                        <%}%>
                </div>
                <div class="swiper-pagination tab-page"></div>
            </div>
        </div>
        <%}%>
</div>